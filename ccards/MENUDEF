AddOptionMenu "BIO_Options"
{
	StaticText ""
	StaticText "$BIOCC_OPTCAT"
	Option "$BIOCC_OPTLBL_ENABLED", "BIOCC_enabled", "OnOff"
	BIO_Tooltip ""
	Slider "$BIOCC_OPTLBL_LVALMULTI", "BIOCC_lvalmulti", 0.01, 1.0, 0.01, 2
	BIO_Tooltip "$BIOCC_OPTTIP_LVALMULTI"
	Slider "$BIOCC_OPTLBL_CHAOSMULTI", "BIOCC_chaosmulti", 1.0, 10.0, 0.01, 2
	BIO_Tooltip "$BIOCC_OPTTIP_CHAOSMULTI"
	Slider "$BIOCC_OPTLBL_MASTERMULTI", "BIOCC_mastermulti", 1.0, 10.0, 0.01, 2
	BIO_Tooltip "$BIOCC_OPTTIP_MASTERMULTI"
}
