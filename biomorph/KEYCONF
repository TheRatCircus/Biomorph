addkeysection "$BIO_MODTITLE" Biomorph

// Controls

addmenukey "$BIO_KB_DROPWEAP" "use BIO_WeaponDrop"
defaultbind Y "use BIO_WeaponDrop"

addmenukey "$BIO_KB_WEAPMODMENU" "event bio_weapmodmenu"
defaultbind M "event bio_weapmodmenu"

// Debug event aliases

alias bio_help_ "event bio_help"
alias bio_lootdiag_ "event bio_lootdiag"
alias bio_weapdiag_ "event bio_weapdiag"
alias bio_monsval_ "event bio_monsval"
alias bio_lootsim_ "event bio_lootsim"
alias bio_weapserialize_ "event bio_weapserialize"

alias bio_lootcoreregen_ "netevent bio_globalregen 0"
alias bio_weaplootregen_ "netevent bio_globalregen 1"
alias bio_mutalootregen_ "netevent bio_globalregen 2"
alias bio_genelootregen_ "netevent bio_globalregen 3"
alias bio_opmoderegen_ "netevent bio_globalregen 4"
alias bio_morphregen_ "netevent bio_globalregen 5"
