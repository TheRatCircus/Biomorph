addkeysection "$BIOM_MODTITLE" biom_Section

// Controls ////////////////////////////////////////////////////////////////////

addmenukey "$BIOM_KB_MUTMENU" "event biom_console 1"
defaultbind M "event biom_console 1"

// Debug event aliases /////////////////////////////////////////////////////////

// Prints a list of all available debug event aliases.
alias biom_help "event biom_console 0"
